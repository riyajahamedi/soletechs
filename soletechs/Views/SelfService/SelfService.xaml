<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="soletechs.Views.SelfService"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="{Binding Title}"
